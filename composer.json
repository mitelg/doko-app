{
    "name": "mt/doko.app",
    "license": "MIT",
    "type": "project",
    "description": "small web app to save points for players of the german card game 'Doppelkopf'",
    "keywords": ["symfony", "doppelkopf"],
    "require": {
        "php": "^7.2.0",
        "ext-iconv": "*",
        "doctrine/collections": "1.6.4",
        "doctrine/doctrine-bundle": "2.0.6",
        "doctrine/doctrine-migrations-bundle": "2.1.2",
        "doctrine/orm": "2.7.0",
        "knplabs/knp-paginator-bundle": "5.0.0",
        "knplabs/knp-components": "2.3.1",
        "sensio/framework-extra-bundle": "5.5.3",
        "symfony/apache-pack": "1.0.1",
        "symfony/asset": "5.0.2",
        "symfony/config": "5.0.2",
        "symfony/console": "5.0.2",
        "symfony/dependency-injection": "5.0.2",
        "symfony/doctrine-bridge": "5.0.2",
        "symfony/dotenv": "5.0.2",
        "symfony/flex": "1.6.0",
        "symfony/form": "5.0.2",
        "symfony/framework-bundle": "5.0.2",
        "symfony/http-foundation": "5.0.2",
        "symfony/http-kernel": "5.0.2",
        "symfony/routing": "5.0.2",
        "symfony/translation": "5.0.2",
        "symfony/translation-contracts": "2.0.1",
        "symfony/twig-bundle": "5.0.2",
        "symfony/validator": "5.0.2",
        "symfony/yaml": "5.0.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "2.16.1",
        "kubawerlos/php-cs-fixer-custom-fixers": "1.17.0",
        "jakub-onderka/php-parallel-lint": "1.0.0",
        "jakub-onderka/php-var-dump-check": "0.3.0",
        "phpstan/phpstan": "0.12.3",
        "sensiolabs/security-checker": "^6.0",
        "symfony/error-handler": "5.0.2",
        "symfony/stopwatch": "5.0.2",
        "symfony/web-profiler-bundle": "5.0.2",
        "vimeo/psalm": "3.8.1"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "security-checker security:check": "script",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts",
            "ln -sf ../../bin/pre-commit.sh .git/hooks/pre-commit"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01C50GDEDXDFSYH6FBWZCZYHPB",
            "allow-contrib": false
        }
    }
}
